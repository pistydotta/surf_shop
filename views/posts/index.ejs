<% layout('layouts/boilerplate') -%>
<h1>Posts Index</h1>
<% posts.forEach(function(post){ %>
    <div>
        <h2><%= post.title %></h2>
        <p>Price = <%= post.price %></p>
        <div><%= post.description %></div>
        <div><%= post.location %></div>
        <div><a href="/posts/<%= post.id %>">View More</a></div>
    </div>
<% }) %>
